QPushButton {
    color: rgb(239,240,241);
    background-color: #005A89;
    
    border: none;
    /* border: 1px solid rgb(46, 52, 54); */
    border-radius: 0px;
    min-height: 32px;
    min-width: 72px;
    padding-left: 4px;
    padding-right: 4px;
    outline: 0px;
    /* outline-style: dotted; */
    /* outline-color: white; */
    /* outline-radius: 4px; */
    /* outline-offset: 2px; */

}

QPushButton:hover {
    background-color: rgb(61,174,233);
}

QPushButton:pressed {
    background-color:  rgb(49,54,59);
}

QPushButton:disabled {
    color: rgb(139,140,141);
    background: rgb(59,64,69);
}
