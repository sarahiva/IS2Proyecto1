
* {
    
    background-color: #EFEFEF;
    color: rgb(35,38,39);
}
QDialogButtonBox {
    dialogbuttonbox-buttons-have-icons: 0;
}
QLineEdit,
QSpinBox,
QDoubleSpinBox,
QComboBox {
    height: 28px;
}



QTableView:focus,
QListView:focus,
QLineEdit:focus,
QSpinBox:focus,
QDoubleSpinBox:focus,
QComboBox:focus {
    border: 2px solid rgb(61,174,233);
    border-radius: 4px;
}

